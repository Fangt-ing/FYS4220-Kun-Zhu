# input pin setup for slide swtiches
set_location_assignment PIN_C10 -to sw[0]
set_location_assignment PIN_C11 -to sw[1]
set_location_assignment PIN_D12 -to sw[2]
set_location_assignment PIN_C12 -to sw[3]

# output pin setup for led
set_location_assignment PIN_A8 -to led[0]
set_location_assignment PIN_A9 -to led[1]
set_location_assignment PIN_A10 -to led[2]
set_location_assignment PIN_B10 -to led[3]

# output pin setup for 7-segment
set_location_assignment PIN_c14 -to seven[0]
set_location_assignment PIN_e15 -to seven[1]
set_location_assignment PIN_c15 -to seven[2]
set_location_assignment PIN_c16 -to seven[3]